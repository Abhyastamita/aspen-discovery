{
  "expo": {
    "name": "Aspen Discovery Mobile App",
    "slug": "aspen_discovery_mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "21.08.0",
    "orientation": "portrait",
    "icon": "./assets/aspenNoTransparency.png",
    "splash": {
      "image": "./assets/aspenLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f373a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.aspendiscovery.mobile",
      "supportsTablet": true,
      "icon": "./assets/aspenLogo.png"
    },
    "android": {
      "package": "org.aspendiscovery.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/aspenLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./assets/aspenLogo.png"
    },
    "web": {
      "favicon": "./assets/aspenLogo.png"
    }
  }
}
