{
  "expo": {
    "name": "Aspen Mobile App",
    "slug": "APL",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/aspenNoTransparency.png",
    "splash": {
      "image": "./assets/aspenLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f373a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.ajaxlibrary.aplApp",
      "icon": "./assets/aspenLogo.png"
    },
    "android": {
      "package": "ca.ajaxlibrary.aplApp",
      "versionCode": 8,
      "permissions": [],
      "icon": "./assets/aspenLogo.png"
    }
  }
}
