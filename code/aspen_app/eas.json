{
  "build": {
	"production": {
	  "node": "16.16.0",
	  "yarn": "1.22.19",
	  "distribution": "store",
	  "channel": "production",
	  "env": {
		"APP_ENV": "{{APP_ENV}}"
	  }
	},
	"beta": {
	  "extends": "production",
	  "channel": "beta"
	},
	"beta-qa": {
	  "extends": "production",
	  "channel": "beta-qa",
	  "ios": {
		"simulator": true
	  }
	},
	"development": {
	  "extends": "production",
	  "channel": "dev",
	  "developmentClient": true,
	  "distribution": "internal",
	  "ios": {
		"simulator": true
	  }
	}
  },
  "submit": {
	"release": {
	  "android": {
		"track": "production",
		"releaseStatus": "draft"
	  },
	  "ios": {
		"appleId": "{{DEV_APPLE_ID}}",
		"ascAppId": "{{DEV_APP_ID}}",
		"appleTeamId": "{{DEV_TEAM_ID}}"
	  }
	},
	"beta": {
	  "extends": "release",
	  "android": {
		"track": "beta"
	  }
	}
  }
}